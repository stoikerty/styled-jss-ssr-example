{
  "extends": ["airbnb", "dev-toolkit", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": "off",
    "react/forbid-prop-types": ["off"],
    "react/require-default-props": "off",
    "react/jsx-curly-brace-presence": "off"
  }
}
